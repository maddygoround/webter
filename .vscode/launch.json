    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program Nodejs",
            "program": "${workspaceFolder}/server/app.js",
            "request": "launch",
            "runtimeVersion": "14",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        

        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        }
    ]
}